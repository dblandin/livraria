{
  "version": 2,
  "routes": [
    {
      "src": "/til/graphql",
      "methods": ["POST", "GET"],
      "dest": "/til/index.js"
    },
    {
      "src": "/graphql",
      "methods": ["POST", "GET"],
      "dest": "/api/index.js"
    },
    {
      "src": "/gateway",
      "methods": ["POST", "GET"],
      "dest": "/gateway/index.js"
    }
  ],
  "builds": [{ "src": "**/*.js", "use": "@now/node" }]
}
